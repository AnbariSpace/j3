{
  "name": "@anbari/j3",
  "version": "0.0.0",
  "description": "Yet another Amazon S3 server implementation",
  "keywords": ["s3", "object-storage", "rest-api"],
  "type": "module",
  "main": "./build/index.js",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "~16.11",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "~5.14",
    "@typescript-eslint/parser": "~5.14",
    "eslint": "~8.11",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.1",
    "jest": "^28.1",
    "node-mocks-http": "^1.11.0",
    "prettier": "~2.5",
    "rimraf": "~3.0",
    "ts-jest": "^28.0",
    "typescript": "^4.6"
  },
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --unhandled-rejections=strict --enable-source-maps build/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "check-disk-space": "^3.3.0",
    "commander": "^9.2.0",
    "express": "^4.18.1",
    "express-validation": "^4.1.0",
    "fast-xml-parser": "^4.0.7",
    "fastify": "^4.0.0-rc.3",
    "lmdb": "^2.3.10",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0",
    "tsyringe": "4.6",
    "winston": "^3.7.2",
    "xml2js": "^0.4.23"
  }
}
